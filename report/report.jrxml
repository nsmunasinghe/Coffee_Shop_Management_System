	cafe_shop.reciept.total,
	cafe_shop.reciept.date,
	cafe_shop.reciept.emp_username
FROM cafe_shop.customerDTO
	INNER JOIN cafe_shop.reciept ON
	 cafe_shop.customerDTO.customer_id = cafe_shop.reciept.customer_id
